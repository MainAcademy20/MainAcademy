class Fibo():

   def __init__(self, n):
       self.n = int(n)  #howmany
       self.a = 0  # start argument 1 int
       self.b = 1  # second argument


   def __iter__(self):
       #  Return an object \n
       #  self (whose type is Fibonacci) is such an object:
       return self



   def __next__(self):

       if self.n == 0:
          raise StopIteration
       self.n -= 1
       sumab = self.a + self.b
       self.a = self.b
       self.b = sumab
       return sumab


n = input("Input Fibonacci max: ")
f = Fibo(n)
for number in f:
   print(number)
